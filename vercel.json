{
  "version": 2,
  "builds": [
    { "src": "app.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "app.py" }
  ],
  "functions": {
    "api/upload": {
      "maxDuration": 10,
      "memory": 128
    }
  },
  "limits": {
    "maxUploadFileSize": 10
  }
}
